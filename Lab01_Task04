        "def dedupe_v1(x):\n",
        "  y = []\n",
        "  for i in x:\n",
        "    if i not in y:\n",
        "      y.append(i)\n",
        "  return y\n",
        "\n",
        "#this one uses sets\n",
        "def dedupe_v2(x):\n",
        "    return list(set(x))\n",
        "\n",
